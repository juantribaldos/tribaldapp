  <form  method="get" action="/prendas/<%= prenda.id %>/codigo"> 
    Lugar: <%= prenda.lugar %> <p>
    <input type="text" name="codigo" value="Codigo"/>
    <input type="submit" value="Enviar">
  </form>
  
  <p><a href="/prendas/<%= prenda.id %>/notas/new"><button>Nueva nota</button></a></p>
  
  <p><strong>Notas:</strong></p>
	<%for(index in prenda.Notas){ %>
		<%if( prenda.Notas[index].publicado || session.user){%>
			<p>- <%=prenda.Notas[index].informe%></p>
		<%if(session.user && !prenda.Notas[index].publicado) {%>
	<a href="/prendizes/<%= prenda.id %>/notas/<%=prenda.Notas[index].id%>/publish"><button>Publicar</button></a></p>
	<%}%>
<%}%>
		
      <p>- <%=prenda.Notas[index].informe %></p>		  		
	<%}%>
